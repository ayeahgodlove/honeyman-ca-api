  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/app.ts",
    "build": "rimraf ./build && tsc",
    "start:build": "npm run build && node build/app.js",
    "db:create": "sequelize-cli db:create",
    "init:db": "npx sequelize init --force",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migration:gen": "ts-node -T -r tsconfig-paths/register ./migration-builder.ts",
    "test": "mocha --check-leaks -r ts-node/register -r tsconfig-paths/register \"src/tests/**/*.spec.ts\"",
    "generate:seed": "sequelize seed:generate --name",
    "seed": "sequelize db:seed:all",
    "generate:seed:file": "sequelize-cli db:seed --seed"
  },
